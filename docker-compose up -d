volumes:
 dbdata:

services:
 mysql:
  image: mysql:5.7
  container_name: mysql
  volumes:
    - dbdata:/var/lib/mysql
  restart: always
  environment:
    - MYSQL_ROOT_PASSWORD=Ubuntu1234$
    - MYSQL_DATABASE=cachengo
    - MYSQL_USER=root
    - MYSQL_PASSWORD=Ubuntu1234$
  ports:
    - "33061:3306"